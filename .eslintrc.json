{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "eqeqeq": "error",
    "max-len": ["error", { "code": 120 }]
  },
  "env": {
    "node": true,
    "es6": true
  }
}
