{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "eqeqeq": "error",
    "max-len": ["error", { "code": 300 }]
  },
  "env": {
    "es2020": true,
    "node": true,
    "es6": true
  }
}
